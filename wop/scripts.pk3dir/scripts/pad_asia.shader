//===============================
//alphas
//===============================

textures/pad_teehouse/bambuspinsel
{
     cull disable
     surfaceparm alphashadow
     surfaceparm trans
     surfaceparm nomarks
     tessSize 64


        {
                map textures/pad_teehouse/bambuspinsel.tga
                alphaFunc GE128
		depthWrite
		rgbGen identity
        }
        {
		map $lightmap
		rgbGen identity
		blendFunc filter
		depthFunc equal
	}

}


textures/pad_teehouse/kirschbluete
